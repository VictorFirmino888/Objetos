class Animal{
    constructor(nome,idade){
        this.nome = nome;
        this.idade = idade;        
    }

    mover(){
        console.log(`${this.nome} está se movendo.`);
}
}

class Cachorro extends Animal{
    constructor(nome,idade, raca){
        super(nome,idade,)
        this.raca = raca;
    }
    
fazerBarulho(){
    return `${this.nome} (raça ${this.raca}) faz latidos.`;
}

buscar(){
    console.log(`${this.nome} está buscando.`);
}
}


var meuCachorro =  new Cachorro('zé', 2, 'Labrador');
console.log(meuCachorro.fazerBarulho());
meuCachorro.mover();
meuCachorro.buscar();

